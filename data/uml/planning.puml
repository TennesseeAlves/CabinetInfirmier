@startuml
package planning{
  class Planning{
    week : Week
    holidays : Holidays
    week : Week
  }

  class Week{
    date : date
    tutorial : string
    lecture : string
  }

  class Holidays{
    date : date
  }

  Planning *-- Week
  Planning *-- Holidays
}

package XMLSchema{
  class string
  class date
}
@enduml